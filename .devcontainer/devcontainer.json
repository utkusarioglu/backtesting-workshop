{
  "name": "backtesting-workshop.workshops.utkusarioglu.com",
  "dockerComposeFile": [
    "../.docker/docker-compose.dev.yml",
    "docker-compose.yml"
  ],
  "service": "backtesting-workshop",
  "workspaceFolder": "/utkusarioglu-com/workshops/backtesting-workshop",
  "customizations": {
    "vscode": {
      "settings": {
        "workbench.colorCustomizations": {
          "activityBar.background": "#003d14",
          "activityBar.foreground": "#ffffff"
        }
      },
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter",
        "ms-python.black-formatter",
        "ms-python.vscode-pylance",
        "tamasfe.even-better-toml",
        "ms-python.flake8"
      ]
    }
  },
  "postCreateCommand": "scripts/post-create-command.sh"
}
